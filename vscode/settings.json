{
    "telemetry.telemetryLevel": "off",
    "editor.minimap.enabled": false,
    "python.venvPath": "~/venvs",
    "editor.fontFamily": "'Source Code Pro",
    "editor.fontLigatures": true,
    "editor.formatOnSave": true,
    "files.insertFinalNewline": true,
    "files.trimTrailingWhitespace": true,
    "clang-format.executable": "${workspaceRoot}/.clang_format",
    "emmet.includeLanguages": {
        "javascript": "javascriptreact"
    },
    "window.menuBarVisibility": "toggle",
    "workbench.activityBar.visible": false,
    "git.confirmSync": false,
    "gitlens.advanced.messages": {
        "suppressShowKeyBindingsNotice": true
    },
    "materialTheme.fixIconsRunning": false,
    "editor.multiCursorModifier": "alt",
    "cSpell.enabled": false,
    "python.linting.pylintUseMinimalCheckers": true,
    "projectManager.git.baseFolders": [
        "$home/projects"
    ],
    "terminal.integrated.commandsToSkipShell": [
        "-workbench.action.quickOpen"
    ],
    "[javascript]": {
        "editor.defaultFormatter": "vscode.typescript-language-features"
    },
    "python.jediEnabled": false,
    "vs-kubernetes": {
        "vs-kubernetes.minikube-path": "${userHome}/.vs-kubernetes/tools/minikube/linux-amd64/minikube",
        "vs-kubernetes.draft-path": "${userHome}/.vs-kubernetes/tools/draft/linux-amd64/draft",
        "vs-kubernetes.helm-path": "${userHome}/.vs-kubernetes/tools/helm/linux-amd64/helm"
    },
    "vim.easymotion": true,
    "vim.useSystemClipboard": true,
    "vim.normalModeKeyBindingsNonRecursive": [
        {
            "before": [
                "<space>"
            ],
            "commands": [
                "vspacecode.space"
            ]
        },
        {
            "before": [
                ","
            ],
            "commands": [
                "vspacecode.space",
                {
                    "command": "whichkey.triggerKey",
                    "args": "m"
                }
            ]
        }
    ],
    "vim.visualModeKeyBindingsNonRecursive": [
        {
            "before": [
                "<space>"
            ],
            "commands": [
                "vspacecode.space"
            ]
        },
        {
            "before": [
                ","
            ],
            "commands": [
                "vspacecode.space",
                {
                    "command": "whichkey.triggerKey",
                    "args": "m"
                }
            ]
        }
    ],
    "workbench.colorTheme": "Better Solarized Dark"
}
